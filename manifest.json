{
  "manifest_version": 3,
  "name": "Four Buttons Starter",
  "version": "1.0.0",
  "description": "Pop up with 4 buttons",
  "action": {
    "default_title": "Four Buttons",
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "notifications",
    "scripting",
    "activeTab",
    "storage"
  ],
  "icons": {
    "128": "icons/sched_tasks.png",
    "48": "icons/sched_tasks.png"
  }
}
